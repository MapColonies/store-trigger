{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": true,
      "url": "http://localhost:4318/v1/traces"
    },
    "metrics": {
      "enabled": true,
      "buckets": [0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1, 2.5, 5, 10, 15, 50, 250, 500]
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "S3": {
    "accessKeyId": "minio",
    "secretAccessKey": "minio",
    "endpointUrl": "http://127.0.0.1:9000",
    "bucket": "3dtiles-dest",
    "region": "us-east-1",
    "forcePathStyle": true,
    "sslEnabled": false,
    "maxAttempts": 3
  },
  "NFS": {
    "pvPath": "/Path/To/Models"
  },
  "ingestion": {
    "provider": "NFS",
    "blackList": ["tar", "zip", "rar", "7z"]
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "ingestion": {
      "jobType": "Ingestion_New_3D",
      "taskType": "tilesCopying",
      "batches": 100
    },
    "delete": {
      "jobType": "Delete_3D",
      "taskType": "deleteModel"
    }
  },
  "maxConcurrency": 5
}
